# AumOS Integration Test Environment
# Copy to .env and adjust for your local setup

# Services base URL (override if services run on non-default ports)
AUMOS_SERVICES_URL=http://localhost

# PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=aumos
POSTGRES_PASSWORD=aumos_dev
POSTGRES_DB=aumos

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Kafka
KAFKA_BOOTSTRAP_SERVERS=localhost:9092

# Keycloak
KEYCLOAK_BASE_URL=http://localhost:8080
KEYCLOAK_REALM=aumos
KEYCLOAK_CLIENT_ID=aumos-test-service
KEYCLOAK_CLIENT_SECRET=test-secret-for-integration

# MinIO
MINIO_ENDPOINT=http://localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
